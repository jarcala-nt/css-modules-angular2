{
  "name": "css-modules-angular2",
  "version": "0.0.0",
  "description": "A working demo of CSS Modules, using Webpack's css-loader in module mode and angular2",
  "main": "",
  "scripts": {
    "webpack": "webpack",
    "clean": "rimraf node_modules doc",
    "clean:dist": "rimraf dist",
    "clean-install": "npm run clean && npm install",
    "clean-start": "npm run clean && npm start",
    "build": "npm run build:dev",
    "prebuild:dev": "npm run clean:dist",
    "build:dev": "webpack --progress --profile --colors --display-error-details --display-cached",
    "watch": "webpack --watch",
    "server:dev": "webpack-dev-server --inline --progress --profile --colors --watch --display-error-details --display-cached  --content-base src/",
    "start": "npm run server:dev",
    "typings-install": "typings install",
    "postinstall": "npm run typings-install"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/joaogarin/css-modules-angular2.git"
  },
